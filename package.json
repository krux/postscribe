  "config": {
    "ports": {
      "cdn": "8080"
    }
  },
  "babel": {
    "presets": [
      "es2015-loose"
    ],
    "plugins": [
      "transform-es3-member-expression-literals",
      "transform-es3-property-literals",
      "transform-object-assign"
    ],
    "comments": true,
    "compact": false
  },
  "scripts": {
    "start": "gulp serve",
    "clean": "gulp clean",
